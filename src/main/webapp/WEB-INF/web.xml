<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <display-name>Acceuil</display-name>
        <servlet-name>Acceuil</servlet-name>
        <servlet-class>com.siteStreaming.SiteStreaming.Acceuil.Acceuil</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Acceuil</servlet-name>
        <url-pattern>/Acceuil</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <display-name>Inscription</display-name>
        <servlet-name>Inscription</servlet-name>
        <servlet-class>com.siteStreaming.SiteStreaming.Acceuil.Inscription</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Inscription</servlet-name>
        <url-pattern>/Acceuil/Inscription</url-pattern>
    </servlet-mapping>

    <servlet>
        <display-name>Profil</display-name>
        <servlet-name>Profil</servlet-name>
        <servlet-class>com.siteStreaming.SiteStreaming.ProfilUser.Profil</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Profil</servlet-name>
        <url-pattern>/Profil</url-pattern>
    </servlet-mapping>

    <servlet>
        <display-name>PageWebAdministration</display-name>
        <servlet-name>PageWebAdministration</servlet-name>
        <servlet-class>com.siteStreaming.SiteStreaming.PageWebAdmin.PageWebAdministration</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PageWebAdministration</servlet-name>
        <url-pattern>/AdminWeb</url-pattern>
    </servlet-mapping>

</web-app>